<tr>
  <td><%= serial_number; %></td>
  <td><%= result.name; %></td>
  <td><%= result.userId; %></td>
  <td><%= result.email; %></td>
  <td><%= result?.phone || 'None'; %></td>
  <td><%= result?.fullAddress || 'None'; %></td>
  
  <td><%= new Date(result.createdAt).toLocaleString(); %></td>
  <td>
    <a href="/admin/portal/users/list/<%= result._id; %>" class="btn btn-info">View</a>
  </td>
</tr>